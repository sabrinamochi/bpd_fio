<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}

</head>

<body>

	{{> base/header 'sticky'}}

	<main id='content'>

		{{> story/intro }}

		<section class="warren">
			{{> story/warren}}
		</section>

		<section class='fio-explain'>
			{{> story/fio-explain}}
		</section>

		<section class='declined'>
			{{> story/declined}}
		</section>

		<section class='black'>
			{{> story/black}}
		</section>

		<section class='support-fio'>
			{{> story/support-fio}}
		</section>

		<section class='scatter-section'>
			{{> story/scatter}}
		</section>

		<section class='map-section'>
			{{> story/map1}}
		</section>

		<section class='map-section'>
			{{> story/map2}}
		</section>

		<section class='rhone'>
			{{#each doc.rhone}}
				<div>
					<p>{{value}}</p>
				</div>
			{{/each}}
		</section>

		<section class='select-person-section'>
			{{> story/select-person}}
		</section>

		<section id='conclude'>
			{{#each doc.conclude}}
				<div>
					<p>{{value}}</p>
				</div>
			{{/each}}
		</section>

	</main>

	{{> base/footer }}


	<script src='{{basepath}}assets/scripts/d3.v6.0.0.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>